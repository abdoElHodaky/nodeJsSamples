<script type="text/javascript">
	if (!session.client){session.setItem('client',"<%=Client%>")}
	cart={};
	$(".btn.btn-warning.fa-plus").click((event)=>{
		event.preventDefault();
		a=$(".product-grid").has(event.target);
		if (parseInt(a.attr("q"))>5) {
			diff=parseInt(a.attr("q"))-5;
			a.attr("q",parseInt(a.q)-diff);
		}
		else{
		a.attr('q',parseInt(a.attr('q'))+1);
		}
			if (a.attr('Incart')=='true') {
				cart[a.find('h4').text().trim()]={q:a.attr('q'),img:a.find('img').attr('src'),price:a.find('p.price').text().trim(),s:a.find("input[type=hidden]").val()};
			}
			else{
				delete cart[a.find('h4').text().trim()];
			}
			if (!session.oproducts) 
			{
					session.setItem('oproducts',"{}");
			}
			else
			{
			Scart=JSON.parse(session.oproducts);
			for(var i in cart){
				for(var j in Scart){
					if (i!=j) {
						cart[j]=Scart[j];
					}
					else{break;}
				}
			}
		}
			session.setItem('oproducts',JSON.stringify(cart));
	})
	$('[for^=check]').click((event)=>{
		a=$(".product-grid").has(event.target);
		if (a.attr('Incart')=='false') {
			a.attr('Incart','true');
		}
		else{
			a.attr('Incart','false');
		}
	});
	$("a[def=logout]").click((event)=>{
		$(event.target).attr('href',"/logout?log="+CryptoJS.HmacSHA1(Math.random().toString(),'<%=Client%>').toString(CryptoJS.enc.Hex));
		session.clear();
	});
</script>
